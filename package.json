{
	"name": "csvtojson-converter",
	"displayName": "CSV to JSON Converter",
	"description": "Convert CSV files to JSON and vice versa",
	"version": "1.0.0",
	"publisher": "Chukwuamaka Osuji",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/Chukwuamaka/csvtojson-converter.git"
	},
	"bugs": {
		"url": "https://github.com/Chukwuamaka/csvtojson-converter/issues"
	},
	"homepage": "https://github.com/Chukwuamaka/csvtojson-converter",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"json",
		"csv",
		"convert"
	],
	"activationEvents": [
		"onCommand:csv.toJSON",
		"onCommand:json.toCSV"
	],
	"main": "./extension.js",
	"contributes": {
		"keybindings":[
			{
				"command": "csv.toJSON",
				"key": "ctrl+alt+j",
				"mac": "cmd+option+j",
				"when": "editorTextFocus"
			},
			{
				"command": "json.toCSV",
				"key": "ctrl+alt+c",
				"mac": "cmd+option+c",
				"when": "editorTextFocus"
			}
		],
		"commands": [
			{
				"command": "csv.toJSON",
				"title": "Convert CSV to JSON"
			},
			{
				"command": "json.toCSV",
				"title": "Convert JSON to CSV"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.46.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
